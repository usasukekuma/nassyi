#include<stdio.h>
#include<math.h>
#include<process.h>
void main()
{
	FILE *fp_output;
	int i;
	double x,xsin,xcos;
	if((fp_output=fopen("data01.dat","w"))==NULL){
		printf("NOT OPEN FILE ouput-file\n");
		exit(1);
	}
	fprintf(fp_output,"　度　　　sin　　　　cos\n");
	for(i=0;i<=180;i+=15){
		x=(double)i/180*3.14159;
		xsin=sin(x);
		xcos=cos(x);
		fprintf(fp_output,"　%3d　　%.4lf　　%.4lf\n",i,xsin,xcos);
	}
	fclose(fp_output);
}
